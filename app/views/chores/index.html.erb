<section class="todoapp">
	<header class="header">
		<h1>Chores</h1>
		<input class="new-todo" placeholder="Lets make this house sparkle!" autofocus>
	</header>

	<section class="main">
		<ul class="todo-list">
			
			<li>
				<div class="view">
					<label><%= link_to chore.name, chore_path(chore) %></label>
				</div>
			</li>
			<% end %>
		</ul>
	</section>
	<footer class="footer">
	</footer>
</section>
